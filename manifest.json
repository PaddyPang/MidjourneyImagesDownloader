{
    "name": "Midjourney Images Batch Downloader",
    "description": "This is an extension for downloading images when using Midjourney in Discord, very simple and easy to use",
    "version": "1.0.1",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "assets/icon.png",
            "19": "assets/icon.png",
            "20": "assets/icon.png",
            "32": "assets/icon.png",
            "38": "assets/icon.png",
            "40": "assets/icon.png"
        }
    },
    "icons": {
        "128": "assets/icon.png",
        "16": "assets/icon.png",
        "32": "assets/icon.png",
        "48": "assets/icon.png",
        "64": "assets/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://discord.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}